<Window x:Class="WPFConverter.ReSizeReturn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFConverter"
        mc:Ignorable="d"
        Title="ReSizeReturn" Height="150" Width="200">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="14,15,0,0" TextWrapping="Wrap" Text="Width:" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="12,39,0,0" TextWrapping="Wrap" Text="Height:" VerticalAlignment="Top"/>
        <TextBox x:Name="tbWidth" HorizontalAlignment="Left" Margin="72,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="68" Height="19"/>
        <TextBox x:Name="tbHeight" HorizontalAlignment="Left" Margin="72,37,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="68" Height="19"/>
        <Button Name="btnOK" Content="OK" HorizontalAlignment="Center" Margin="0,61,0,0" VerticalAlignment="Top" Height="19" Width="25" Click="btnOK_Click"/>

    </Grid>
</Window>
